# AWS CDK Stack Configuration
# Copy this file to .env and update with your values

# The name for your CDK stack (e.g., MyCompanyMonitoringStack)
CDK_STACK_NAME=

# AWS Route 53 Hosted Zone Configuration
# Your Route 53 hosted zone ID (found in AWS Console > Route 53 > Hosted zones)
CDK_HOSTED_ZONE_ID=

# The domain name for your hosted zone (e.g., example.com)
CDK_HOSTED_ZONE_DOMAIN_NAME=

# The subdomain where Bugsink will be accessible (e.g., bugsink)
# This will create {subdomain}.{domain} (e.g., bugsink.example.com)
CDK_BUGSINK_SUBDOMAIN=

# The subdomain where Uptime Kuma will be accessible (e.g., uptime)
# This will create {subdomain}.{domain} (e.g., uptime.example.com)
CDK_UPTIME_SUBDOMAIN=
